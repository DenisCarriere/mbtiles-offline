{
  "name": "mbtiles-offline",
  "version": "3.0.4",
  "description": "MBTiles SQLite3 binding (for offline environments)",
  "main": "index.js",
  "types": "index.d.js",
  "files": [
    "index.js",
    "index.d.ts",
    "utils",
    "schema"
  ],
  "scripts": {
    "types": "tsc index.d.ts types.ts --lib es6",
    "lint": "standard",
    "pretest": "npm run types && npm run lint",
    "test": "tap test.js utils/utils.test.js --coverage",
    "docs": "documentation readme index.js --shallow --section=API"
  },
  "author": "Denis Carriere <@DenisCarriere>",
  "license": "MIT",
  "keywords": [
    "mbtiles",
    "sqlite3",
    "offline"
  ],
  "dependencies": {
    "@mapbox/tiletype": "^0.3.1",
    "@turf/bbox": "^3.10.3",
    "bbox-dateline": "^1.1.1",
    "debug": "^2.6.3",
    "global-mercator": "^2.6.0",
    "lodash": "^4.17.4",
    "sqlite3-offline": "3.1.8-2"
  },
  "devDependencies": {
    "@types/geojson": "^1.0.0",
    "@types/node": "^7.0.5",
    "coveralls": "^2.11.15",
    "documentation": "^4.0.0-beta.18",
    "fs-extra": "^2.0.0",
    "load-json-file": "^2.0.0",
    "standard": "^8.6.0",
    "tap": "^10.7.0",
    "tape": "^4.7.0",
    "typescript": "^2.1.5",
    "write-json-file": "^2.0.0"
  }
}
